<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Centre" table="centre">
    <indexes>
      <index name="IDX_C6A0EA756186CA22" columns="user_key"/>
    </indexes>
    <id name="idCentre" type="integer" column="id_centre">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="centre_id_centre_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="capacites" type="text" column="capacites" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="userKey" target-entity="Users" fetch="LAZY">
      <join-columns>
        <join-column name="user_key" referenced-column-name="id_user"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
