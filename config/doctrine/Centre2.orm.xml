<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Centre2" table="centre2">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="centre2_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lat" type="float" column="lat" precision="10" scale="0" nullable="false"/>
    <field name="lon" type="float" column="lon" precision="10" scale="0" nullable="false"/>
    <field name="ville" type="string" column="ville" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tel" type="string" column="tel" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adresse" type="string" column="adresse" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="ouverture" type="string" column="ouverture" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
