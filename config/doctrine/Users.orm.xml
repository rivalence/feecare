<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Users" table="users">
    <id name="idUser" type="integer" column="id_user">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="users_id_user_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="nom" type="string" column="nom" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="age" type="smallint" column="age" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="email" type="string" column="email" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mdp" type="string" column="mdp" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="utilisateur" type="string" column="utilisateur" length="15" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
